{
  "name": "geoparquet",
  "version": "0.5.0",
  "description": "A library for reading geospatial data in Apache Parquet format",
  "author": "Hyperparam",
  "homepage": "https://hyperparam.app",
  "keywords": [
    "geo",
    "geojson",
    "geoparquet",
    "geospatial",
    "gis",
    "parquet"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyparam/geoparquet.git"
  },
  "dependencies": {
    "hyparquet": "1.17.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@types/google.maps": "3.58.1",
    "@types/node": "24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.33.0",
    "http-server": "14.1.1",
    "hyparquet-compressors": "1.1.1",
    "rollup": "4.46.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "files": [
    "src",
    "types"
  ],
  "type": "module",
  "types": "types/index.d.ts",
  "main": "src/index.js",
  "exports": {
    "browser": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "default": {
      "types": "./types/node.d.ts",
      "import": "./src/node.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc -p ./tsconfig.lib.json",
    "coverage": "vitest run --coverage --coverage.include=src",
    "lint": "eslint",
    "prepare": "npm run build:types",
    "start": "http-server -a localhost",
    "test": "vitest run"
  }
}
